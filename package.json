{
  "name": "vuex-function-actions",
  "version": "1.0.0",
  "description": "Utilities for Vuex",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "sideEffects": false,
  "scripts": {
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development webpack",
    "build:umd:min": "cross-env NODE_ENV=production webpack && es-check es5 dist/vuex-function-actions.min.js",
    "build": "run-s clean && run-p build:**",
    "clean": "rimraf coverage dist es lib",
    "test": "jest",
    "lint": "eslint .",
    "precommit": "lint-staged"
  },
  "files": [
    "es",
    "lib",
    "dist"
  ],
  "keywords": [
    "vue",
    "vuex",
    "actions"
  ],
  "author": "Simone Busoli <simone.busoli@gmail.com>",
  "bugs": {
    "url": "https://github.com/simoneb/vuex-function-actions/issues"
  },
  "homepage": "https://github.com/simoneb/vuex-function-actions",
  "repository": {
    "type": "git",
    "url": "https://github.com/simoneb/vuex-function-actions.git"
  },
  "license": "MIT",
  "dependencies": {
    "invariant": "^2.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "es-check": "^2.1.0",
    "eslint": "^5.5.0",
    "eslint-config-jest-files": "^0.1.3",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-unicorn-camelcase": "^0.1.1",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "lint-staged": "^7.2.2",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "vuex": "^3.0.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "peerDependencies": {
    "vuex": "^3.0.1"
  },
  "jest": {
    "collectCoverage": true
  },
  "lint-staged": {
    "*.{js,md}": [
      "eslint --fix",
      "git add"
    ]
  }
}
